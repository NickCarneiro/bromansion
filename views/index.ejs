<% 
var max_bros = 0;
if(bros.length > 5){
	max_bros = 5;
} else {
	max_bros = bros.length;
}
for(var i=0; i < max_bros; i++) {%>
	<div class="bromeme">
		<a href="/bro/<%= bros[i].image_uuid %>">
			<img src="/images/captions/<%= bros[i].image_uuid %>.jpg" />
		</a>
	</div>

	<br />
	<div class="fb-comments" data-href="http://bromansion.com?imageuuid=<%= bros[i].image_uuid%>" data-num-posts="2" data-width="470">
	</div>
	<br />

<% } %> 